generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model RefreshToken {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt   DateTime @db.Date @default(now())
  hashedToken String
  id_         String   @map("id")
  revoked     Boolean @default(false)
  updatedAt   DateTime @db.Date @default(now())
  userId      Int
  User        User     @relation(fields: [userId], references: [id_], onDelete: Cascade)
}

model TauxItemBrisage {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @db.Date @default(now())
  id_       Int?      @map("id")
  id_item   Int
  id_server Int
  taux      Int
  item      item     @relation(fields: [id_item], references: [id_])
}

model User {
  id           String         @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    DateTime       @db.Date @default(now())
  email        String
  id_          Int            @unique @map("id")
  password     String
  pseudo       String
  updatedAt    DateTime       @db.Date @default(now())
  refreshToken RefreshToken[]
}

model classs {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  female_colors String
  female_look   String
  id_           Int    @map("id")
  male_colors   String
  male_look     String
  name_de       String
  name_en       String
  name_es       String
  name_fr       String
  name_it       String
  name_pt       String
}

model class_spell {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  class_id Int
  id_      Int    @map("id")
  spell_id Int
}

model classdofus {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  female_colors String
  female_look   String
  id_           Int    @map("id")
  male_colors   String
  male_look     String
  name_de       String
  name_en       String
  name_es       String
  name_fr       String
  name_it       String
  name_pt       String
}

model dungeon {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  id_     Int    @map("id")
  level   Int
  name_de String
  name_en String
  name_es String
  name_fr String
  name_it String
  name_pt String
}

model item {
  id          String            @id @default(auto()) @map("_id") @db.ObjectId
  criteria    String
  desc_de     String
  desc_en     String
  desc_es     String
  desc_fr     String
  desc_it     String
  desc_pt     String
  icon_id     Int
  id_         Int               @unique @map("id")
  item_set_id Int
  level       Int
  name_de     String
  name_en     String
  name_es     String
  name_fr     String
  name_it     String
  name_pt     String
  nuggets     Int
  pet_food_xp Int
  pods        Int
  price       Int
  type_id     Int
  rune_price  runes_prices[]
  taux_item   TauxItemBrisage[]
  item_effect item_effect[]
  item_type   item_type         @relation(fields: [type_id], references: [id_])
}

model item_effect {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  boost        String
  category     Int
  desc_de      String
  desc_en      String
  desc_es      String
  desc_fr      String
  desc_it      String
  desc_pt      String
  effect_id    Int
  element_id   Int
  element_name String
  id_          Int    @map("id")
  item_id      Int
  max          Int
  min          Int
  /// Multiple data types found: Float: 8.8%, Int: 91.2% out of 1000 sampled entries
  power        Json
  rune_item_id Int
  value        Int
  item         item   @relation(fields: [item_id], references: [id_])
}

model item_set {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  id_     Int    @map("id")
  name_de String
  name_en String
  name_es String
  name_fr String
  name_it String
  name_pt String
}

