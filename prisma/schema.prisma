model item_type {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  category   Int
  id_        Int    @unique @map("id")
  name_de    String
  name_en    String
  name_es    String
  name_fr    String
  name_it    String
  name_pt    String
  super_type Int
  item       item[]
}

model job {
  id         String       @id @default(auto()) @map("_id") @db.ObjectId
  icon_id    Int
  id_        Int          @unique @map("id")
  name_de    String
  name_en    String
  name_es    String
  name_fr    String
  name_it    String
  name_pt    String
  job_action job_action[]
}

model job_action {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  icon_id       String
  id_           Int    @map("id")
  ingredientIds String
  is_craft      String
  item_id       Int
  job_id        Int
  level         Int
  name_de       String
  name_en       String
  name_es       String
  name_fr       String
  name_it       String
  name_pt       String
  price         String
  quantities    String
  skill_name_de String
  skill_name_en String
  skill_name_es String
  skill_name_fr String
  skill_name_it String
  skill_name_pt String
  xp            Int
  job           job    @relation(fields: [job_id], references: [id_])
}

model job_craft_ingredient {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  icon_id       String
  id_           Int    @map("id")
  ingredient_id Int
  item_id       Int
  level         String
  name_de       String
  name_en       String
  name_es       String
  name_fr       String
  name_it       String
  name_pt       String
  pods          String
  quantity      Int
  type_id       String
}

model runes_prices {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  id_        Int      @map("id")
  id_intern  Int
  item_id    Int
  price      Int
  saved_date DateTime @db.Date
  server_id  Int
  user_id    Int
  item       item     @relation(fields: [item_id], references: [id_])
}

model server_list {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  en        String
  es        String
  fr        String
  id_       Int    @map("id")
  name_id   Int
  server_id Int
}

model weapon {
  id                     String @id @default(auto()) @map("_id") @db.ObjectId
  ap_cost                Int
  criticalHitBonus       Int
  criticalHitProbability Int
  id_                    Int    @map("id")
  maxCastPerTurn         Int
  min_range              Int
  range                  Int
}
