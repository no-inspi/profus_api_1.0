  ini             String
  item_count      Int
  item_set_id     Int
  pods            String
  pp              String
  pwr_air         String
  pwr_all         String
  pwr_earth       String
  pwr_fire        String
  pwr_pi          String
  pwr_water       String
  res_air         String
  res_crit        String
  res_earth       String
  res_fire        String
  res_neutral     String
  res_per_air     String
  res_per_earth   String
