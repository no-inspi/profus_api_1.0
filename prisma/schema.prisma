model item_set_effect {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  desc_de     String
  desc_en     String
  desc_es     String
  desc_fr     String
  desc_it     String
  desc_pt     String
  effect_id   Int
  id_         Int    @map("id")
  item_count  Int
  item_set_id Int
  max         Int
}

model item_test {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  criteria    String
  desc_de     String
  desc_en     String
  desc_es     String
  desc_fr     String
  desc_it     String
  desc_pt     String
  icon_id     Int
  id_         Int    @map("id")
  item_set_id Int
  level       Int
  name_de     String
  name_en     String
  name_es     String
  name_fr     String
  name_it     String
  name_pt     String
  nuggets     Int
  pet_food_xp Int
  pods        Int
  price       Int
  type_id     String
  //item_type   item_type @relation(fields: [type_id], references: [id_])
